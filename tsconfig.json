{
   "compilerOptions": {
      "lib": [
         "es6",
         "es5"
      ],
      "target": "es5",
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "./build",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true
   },
   "typeRoots": [
      "./node_modules/@types",
      "./src/types"
   ],
   "include": [
      "./src/**/*",
      "./package.json",
   ],
}